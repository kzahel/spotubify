{
    "key": "",
    "name": "Spotubify",
    "short_name": "Spotubify",
    "description": "Watch youtube videos related to your spotify playlists",
    "author": "Kyle Graehl",
    "version": "0.0.1",
    "manifest_version": 2,
    "offline_enabled": true,
    "minimum_chrome_version": "33",
    "app": {
        "background": {
            "scripts": ["web-server-chrome/underscore.js","web-server-chrome/common.js","web-server-chrome/mime.js","web-server-chrome/buffer.js","web-server-chrome/request.js","web-server-chrome/stream.js","web-server-chrome/connection.js","web-server-chrome/webapp.js","web-server-chrome/handlers.js","web-server-chrome/httplib.js","background.js"]
        }
    },
    "sockets": {
       "tcpServer": {
         "listen": ["127.0.0.1:*"]
       }
    },
    "permissions": [
        "webview",
        "https://www.google-analytics.com/",
        "storage",
        "notifications"
    ],
    "icons": { 
        "16": "flvico-16.png",
        "19": "flvico-19.png",
        "32": "flvico-32.png",
        "38": "flvico-38.png",
        "48": "flvico-48.png",
        "64": "flvico-64.png",
        "96": "flvico-96.png",
        "128": "flvico-128.png",
        "256": "flvico-256.png",
        "512": "flvico-512.png"
    }
}
