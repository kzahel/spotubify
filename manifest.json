{
    "key": "",
    "name": "Spotubify",
    "short_name": "Spotubify",
    "description": "Watch youtube videos related to your spotify playlists",
    "author": "Kyle Graehl",
    "version": "0.0.1",
    "manifest_version": 2,
    "offline_enabled": true,
    "minimum_chrome_version": "33",
    "app": {
        "background": {
            "scripts": ["js/web-server-chrome/underscore.js","js/web-server-chrome/common.js","js/web-server-chrome/mime.js","js/web-server-chrome/buffer.js","js/web-server-chrome/request.js","js/web-server-chrome/stream.js","js/web-server-chrome/connection.js","js/web-server-chrome/webapp.js","js/web-server-chrome/handlers.js","js/web-server-chrome/httplib.js","background.js"]
        }
    },
    "sockets": {
       "tcpServer": {
         "listen": ["127.0.0.1:*"]
       }
    },
    "permissions": [
        "webview",
        "identity",
        "alwaysOnTopWindows",
        "https://www.google-analytics.com/",
        "storage",
        "notifications"
    ],
    "icons": { 
        "16": "img/ico-spotubify-16.png",
        "19": "img/ico-spotubify-19.png",
        "32": "img/ico-spotubify-32.png",
        "38": "img/ico-spotubify-38.png",
        "48": "img/ico-spotubify-48.png",
        "64": "img/ico-spotubify-64.png",
        "96": "img/ico-spotubify-96.png",
        "128": "img/ico-spotubify-128.png",
        "256": "img/ico-spotubify-256.png"
    }
}
